{
  "name": "@digdir/designsystemet-react",
  "type": "module",
  "version": "1.5.1",
  "description": "React components for Designsystemet",
  "author": "Designsystemet team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digdir/designsystemet.git"
  },
  "homepage": "https://github.com/digdir/designsystemet/tree/main/packages/react",
  "license": "MIT",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./colors": {
      "types": "./dist/types/colors.d.ts"
    },
    "./react-types": {
      "types": "./dist/react-types.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "pnpm run clean && tsc -b tsconfig.lib.json --emitDeclarationOnly false && rollup -c --bundleConfigAsCjs",
    "clean": "rimraf dist && rimraf tsc-build && rimraf --glob \"*.tsbuildinfo\"",
    "copy-css-to-build": "copyfiles -u 1 ./src/**/*.css ./tsc-build/",
    "types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.3.1 || ^19.0.0",
    "react-dom": ">=18.3.1 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@floating-ui/react": "0.26.23",
    "@navikt/aksel-icons": "^7.29.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@u-elements/u-combobox": "^1.0.1",
    "@u-elements/u-datalist": "^1.0.14",
    "@u-elements/u-details": "^0.1.5",
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@digdir/designsystemet": "workspace:^",
    "@digdir/designsystemet-css": "workspace:^",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "jsdom": "^26.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.1",
    "rollup-plugin-copy": "^3.5.0",
    "storybook": "^9.1.5",
    "tsx": "4.20.5",
    "typescript": "^5.9.2"
  }
}
